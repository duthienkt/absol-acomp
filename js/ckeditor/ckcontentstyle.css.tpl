
.as-ck-alert {
    padding: .75rem 1.25rem;
    margin-bottom: 1rem;
    border: 1px solid transparent;
    border-radius: .188rem;
}

.as-ck-alert.as-variant-notice {
    color: #7b5700;
    background-color: #fbedcc;
    border-color: #fae6b8;
}

.as-ck-alert.as-variant-warning {
    color: #852d19;
    background-color: #ffddd6;
    border-color: #ffd0c5;
}

.as-ck-alert.as-variant-info{
    color: #352c64;
    background-color: #e0ddf2;
    border-color: #d4cfed;
}

.as-ck-alert.as-variant-success {
    color: #2a601f;
    background-color: #dcf1d8;
    border-color: #ceebc8;
}

.cke_contents_ltr ul{
    list-style: none;
}

